{{>header }}
  <div class="container">
    <div class="card text-center">
      <div class="card-header">
        Crear Usuario
      </div>
      <div class="card-body">
        <h5 class="card-title">Crear Usuario</h5>
        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
        <div class="container text-left">
          <form>
            <div class="form-group">
              <label for="numDoc">Número Documento</label>
              <input type="text" class="form-control" id="numDoc" name="numDoc" placeholder="Número de documento">
            </div>
            <div class="form-group">
              <label for="nombre">Nombre</label>
              <input type="text" class="form-control" id="nombre" name="nombre" placeholder="Nombre">
            </div>
            <div class="form-group">
              <label for="correo">Correo</label>
              <input type="text" class="form-control" id="nombre" name="correo" placeholder="Correo electronico">
            </div>
            <div class="form-group">
              <label for="telefono">Telefono</label>
              <input type="text" class="form-control" id="telefono" name="telefono" placeholder="Telefono">
            </div>
          </form>
        </div>
      </div>
      <div class="card-footer text-muted  text-right">
        <button type="button" name="button" class="btn btn-outline-success">Guardar</button>
        <button type="button" name="button" class="btn btn-outline-dark">Cancelar</button>
      </div>
    </div>
  </div>
{{>footer}}
